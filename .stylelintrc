{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
          "tailwind",
          "apply",
          "variants",
          "responsive",
          "screen",
          "extends",
          "import"
      ]
    }],
    "declaration-block-trailing-semicolon": null,
    "no-descending-specificity": null,
    "block-no-empty": null,
    "declaration-empty-line-before": null
  }
}